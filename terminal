# git 초기화
git init

# .gitignore 파일 생성
echo "node_modules/
.next/
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.DS_Store" > .gitignore

# 파일 스테이징
git add .

# 첫 커밋
git commit -m "Initial commit: KMC Admin App setup with Next.js" 